{
    "INITIALS": [
        "W0",
        "M",
        "rh",
        "ra",
        "g",
        "delta",
        "s2",
        "F",
        "a1",
        "a2",
        "a3",
        "BHret",
        "d",
    ],

    // TODO this metadata is not so well defined (and also units???)
    "METADATA": {
        "requires": ["FeHe", "age", "RA", "DEC", "l", "b", "μ"],
        "optional": ["Ndot"]
    },

    // Dataset
    // "requires": This thing is required
    // "choice": One or more of these things is required
    // "optional": This thing may be given or it may not be
    // "collective": If one of these exists, they all must exist

    // TODO also include dataset metadata, all need source, 'm' is optional
    //      MF needs field_unit

    "pulsar*":{
        "requires": {
            "r": {"requires": ["unit"]},
        },
        "optional": {
            "DM": {"requires": ["unit", "error"]}
        },
        "choice": {
            "collective": {
                "P": {"requires": ["unit", "error"]},
                "Pdot": {"requires": ["unit", "error"]},
            },
            "collective": {
                "Pb": {"requires": ["unit", "error"]},
                "Pbdot": {"requires": ["unit", "error"]},
            },
        }
    },


    "velocity_dispersion*":{
        "requires": {
            "r": {"requires": ["unit"]},
            "σ": {"requires": ["unit", "error"]}
        }
    },


    "number_density*":{
        "requires": {
            "r": {"requires": ["unit"]},
            "Σ": {"requires": ["unit", "error"]}
        }
    },


    "proper_motion*":{
        "requires": {
            "r": {"requires": ["unit"]},
        },
        // TODO "choice" might not make it clear that its *at least one of*
        "choice": {
            "PM_R": {"requires": ["unit", "error"]},
            "PM_T": {"requires": ["unit", "error"]},
            "PM_tot": {"requires": ["unit", "error"]},
            "PM_ratio": {"requires": ["error"], "optional": ["unit"]},
        }
    },


    "mass_function*":{
        "requires": {
            "N": {"requires": ["error"]},
            "r1": {"requires": ["unit"]},
            "r2": {"requires": ["unit"]},
            "m1": {"requires": ["unit"]},
            "m2": {"requires": ["unit"]},
            // TODO change the dumb "fields_unit" to just be a unit on the dset
            "fields": {
                "requires": ["unit"],
                // TODO ugly way to document this... but yeah..
                "choice": ["a", "b", "c", "d", "e", "f", "g", "h", "i", "j",
                           "k", "l", "m", "n", "o", "p", "q", "r", "s", "t",
                           "u", "v", "w", "x", "y", "z"]
            }
        }
    },


}